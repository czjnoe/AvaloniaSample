<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="AvaloniaSample.WarningDialog"
             xmlns:vm="using:AvaloniaSample.ViewModels"
             x:DataType="vm:WarningDialogViewModel"
             >
  <StackPanel>
        <TextBlock Text="{Binding Message}" Margin="10"/>
        <Button Content="确认" Command="{Binding ConfirmCommand}" />
        <Button Content="取消" Command="{Binding CancelCommand}" />
    </StackPanel>
</UserControl>
